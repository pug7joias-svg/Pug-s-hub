FOVCircleFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
FOVCircleFrame.Visible = false
FOVCircleFrame.ZIndex = 999
FOVCircleFrame.Parent = FOVGui

local CircleStroke = Instance.new("UIStroke")
